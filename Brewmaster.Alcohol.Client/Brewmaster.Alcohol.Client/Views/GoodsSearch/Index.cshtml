<script>
    var pageIndex = 1;
    var pageSize = 5;
    var total = 0;
    var priceNowNumber = 123;
    $(function () {
        GetGoodsType();
        GetBrandName();
        GetGoodsAll(789);
    });

    //获取价格排序条件
    $("#OrderByPrice").change(function () {
        GetGoodsAll($(this).val());
    });
    function GetGoodsType() {
        $.ajax({
            url: 'http://localhost:50169/api/GoodsType/GetGoodsTypeName',
            success: function (result) {
                var tmphtml = "";
                $("#GetGoodsType").empty();
                for (var i = 0; i < result.length; i++) {
                    tmphtml += "<ul>";
                    tmphtml += "<li>" + result[i].typeName + "</li>";
                    tmphtml += "</ul>";
                }
                $("#GetGoodsType").append(tmphtml);
            }
        })
    }
    function GetBrandName() {
        $.ajax({
            url: 'http://localhost:50169/api/Brand/GetBrandName',
            success: function (result) {
                var tmphtml = "";
                $("#brandAll").empty();
                for (var i = 0; i < result.length; i++) {
                    tmphtml += "<ul>";
                    tmphtml += "<li>" + result[i].brandName + "</li>";
                    tmphtml += "</ul>";
                }
                $("#brandAll").append(tmphtml);
            }
        })
    }
    function GetGoodsAll(priceNow) {
        priceNowNumber = priceNow;
        var name = $("#GoodsName").val();
        $.ajax({
            url: 'http://localhost:50169/api/GoodsAll/GetGoodsAllListPage?PriceNow=' + priceNow + '&GoodsName=' + name + '&pageIndex=' + pageIndex + '&pageSize=' + pageSize,
            success: function (result) {
                var i = 0;
                $("#SearchGoods").empty();
                $(result.goodsAll).each(function() {
                    $("#SearchGoods").append('<li>' +
                        '<img src="' +this.goodsImg +'" />'
                        + '<div>' + this.goodsName+'</div>'
                        +'<div>'+this.priceNow+'</div>'
                        +'</li>');
                    i++;
                    total = result.total;
                });
            }
        })
    }
    
    //首页
    function first() {
        pageIndex = 1;
        GetGoodsAll(priceNowNumber);
    }

    //上页
    function prve() {
        pageIndex = pageIndex > 1 ? pageIndex - 1 : 1;
        GetGoodsAll(priceNowNumber);
    }

    //下页
    function next() {

        var nums = Math.ceil(total / pageSize);
        pageIndex = pageIndex <= nums ? pageIndex + 1 : pageIndex;
        GetGoodsAll(priceNowNumber);
    }

    //尾页
    function last() {
        pageIndex = Math.ceil(total / pageSize);
        GetGoodsAll(priceNowNumber);
    }
</script>