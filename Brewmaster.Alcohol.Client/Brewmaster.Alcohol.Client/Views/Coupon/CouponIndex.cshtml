                              target: sub_request_url,
                              emailAddress: subscribeEmail
                            }, function(data) {
                              if (data != null) {
                                if (data.code == 1) {
                                  alert("订阅促销信息成功。")
                                } else {
                                  if (data.code == 0) {
                                    alert("请输入正确的邮箱")
                                  } else {
                                    if (data.code == 2) {
                                      alert("该Email地址已经订阅过促销信息！")
                                    } else {
                                      if (data.code == 3) {
                                        alert("对不起，订阅失败！")
                                      }
                                    }
                                  }
                                }
                              }
                            }, "JSON")
                          }
