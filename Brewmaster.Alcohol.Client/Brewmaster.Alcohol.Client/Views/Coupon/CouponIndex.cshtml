                      } else {
                        var regemail = /^[\w\-\.]+?[\w\-]+?(\.\w+?)+?$/;
                        if (subscribeEmail.length > 30) {
                          alert("请输入正确的邮箱")
                        } else {
                          if (!subscribeEmail.match(regemail)) {
                            alert("请输入正确的邮箱")
