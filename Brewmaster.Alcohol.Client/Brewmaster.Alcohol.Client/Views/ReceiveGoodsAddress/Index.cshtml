				if($(this).attr('id')&&$(this).attr('id')!='zipcode'&&$(this).attr('id')!='phone')
				{
					c=1;
					var txt=$(this).attr('id');
					$('#'+txt+'TS').show();
					$(this).addClass('borderRed');
					return '';
				}
			}
		})

		$('#allInput option:selected').each(function(i){

			if($(this).val()=='')
			{
				c=1;
