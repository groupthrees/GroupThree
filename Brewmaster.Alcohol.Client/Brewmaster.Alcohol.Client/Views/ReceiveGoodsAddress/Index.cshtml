				$('.controlChange[c=1]').attr('c','0');

				$('.ln_addressBoxTitL[c]').text('添加新地址');
				$('.ln_addressBoxTitL[c]').attr('c','0');
				$('#people,#address,#iphone').val('');
				$('select').each(function(){

					$('option').attr('selected',false)
					$('option[value=0]').attr('selected','selected');

				})

			}
			else
			{
				$('.ln_addressBox1').show();
				str= "<tr><td class='men'><p>"+menVal+"</p></td><td class='adr'><p class='adr-td'><span style='margin-right:5px;' a="+city1+">河北省</span><span style='margin-right:5px;' a="+city2+">保定市</span><span style='margin-right:5px;' a="+city3+">涞水县</span><span style='margin-right:5px;' a="+city4+">九龙镇</span></p><p class='adr-td'>"+adrVal+"</p></td><td class='pho'>"+phoVal+"</td><td class='control'><a href='javascript:;' class='controlChange'>修改</a>  | <a href='javascript:;' class='controlRemove'>删除</a></td></tr>";

				$('.ln_addressBoxTit tbody').append(str);
				$('#people,#address,#iphone').val('');

				var cityAdr=$('.ln_addressBoxTit tr').last().find('.adr');
				cityAdr.find('span').each(function(i){

					var txt=$('select').eq(i).find('option:selected').text();
					$(this).text(txt)

				})

				$('select').each(function(){

					$('option').attr('selected',false)
					$('option[value=0]').attr('selected','selected');

				})


			}

		}

	})

	$('.controlChange').live('click',function(){
