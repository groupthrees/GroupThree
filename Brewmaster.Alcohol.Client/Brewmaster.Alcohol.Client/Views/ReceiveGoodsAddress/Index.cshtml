			$(this).addClass('borderRed');
		}
		else
		{
			$('#addressTS').hide();
			$(this).removeClass('borderRed');
		}
	})

	$('#save').bind('click',function(){              //保存按钮
		var c=0;
		$('#allInput input').each(function(){

			if($(this).hasClass('borderRed'))
			{
				c=1;
				return '';

			}
			else if($(this).val()=='')
