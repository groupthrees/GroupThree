		flag = /^([0-9]{1,}-?[0-9]{1,})$/;
		}
		var value=$(this).val();
		if(value!='' && !flag.test(value))
		{

			$('#phoneTS').show();
			$('#phoneTS').find('span').text('请输入正确的固定电话')
			$(this).addClass('borderRed');
		}
		else
		{

			$(this).removeClass('borderRed');
			$('#phoneTS').hide();
		}
	})

