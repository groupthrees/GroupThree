<script>
    
    $(function () {

        getOrder();


    })
    function getOrder() {
        //var date = {
            
        //    "usersId": 1,
        //    "orderSite": 0,

        //};
        //var val = JSON.stringify(date);
        $.ajax({
            type: "get",
            url: 'http://localhost:50169/api/Indent/GetIndentPageList',
            data: { userId: @ViewBag.UserId, OrderSite:0},
            success: function (result) {
                console.log(result);

                var tmphtml = "";
                $("#await").empty();


              
                for (var i = 0; i < result.length; i++) {
                    tmphtml += "<tr>";
               
                    tmphtml += "<td>" + result[i].addressPerson + "</td>";
                    tmphtml += "<td>" + result[i].practicalMoney + "</br>" + result[i].applyMethod + "</td>";
                    //tmphtml += "<div></div>"
                    //tmphtml += "<div></div>"
                    for (var j = 0; j < result[i].imgs.length; j++) {
                        tmphtml += "<td><img src=" + result[i].imgs[j] + "  width='50' height='50' ></td>";
                    }    
                    tmphtml += "</tr>";
                   
                }
                $("#await").append(tmphtml);
                total = result.total;
            }
        })
    }
   
     </script>